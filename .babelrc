{
    "plugins": [
        "transform-runtime",
        "transform-object-rest-spread",
        "transform-async-to-generator",
        "syntax-trailing-function-commas"
    ],
    "presets": [
        [
            "env",
            {
                "debug": true,
                "loose": false,
                "modules": "commonjs",
                "spec": false,
                "tragets": {
                    "node": "current"
                },
                "useBuiltIns": false
            }
        ]
    ]
}
